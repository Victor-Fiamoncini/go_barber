{
  "expo": {
    "name": "gobarber-mobile",
    "slug": "gobarber-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
		"icon": "./assets/icon.png",
		"entryPoint": "./src/App.tsx",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
